<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<Pane styleClass="root" stylesheets="@../css/lblcontact.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gov.iti.link.presentation.controllers.LabelContactController">
   <children>
      <HBox alignment="CENTER_LEFT" layoutX="-3.0">
         <children>
            <Circle fx:id="circlePic" fill="DODGERBLUE" radius="25.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Circle>
            <Circle fx:id="circlePicStatus" fill="#1fff3d" radius="6.0" stroke="BLACK" strokeType="INSIDE" translateX="-10.0" translateY="17.0">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Circle>
            <VBox alignment="CENTER_LEFT" prefHeight="54.0" prefWidth="144.0" stylesheets="@../css/lblcontact.css">
               <padding>
                  <Insets bottom="5.0" />
               </padding>
               <children>
                  <Label fx:id="lblText" prefHeight="26.0" prefWidth="123.0" style="-fx-background-radius: 15;" text="Name">
                     <font>
                        <Font name="Ebrima Bold" size="20.0" />
                     </font>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </Label>
                  <Text fx:id="txtNumber" opacity="0.52" strokeType="OUTSIDE" strokeWidth="0.0" text="You : bye" translateX="5.0" translateY="10.0">
                     <font>
                        <Font name="System Italic" size="14.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="-5.0" top="-10.0" />
                     </VBox.margin>
                  </Text>
               </children>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </VBox>
         </children>
         <padding>
            <Insets bottom="10.0" top="5.0" />
         </padding>
      </HBox>
      <Label fx:id="lblunSeenMessages" alignment="CENTER" layoutX="199.0" layoutY="18.0" styleClass="notifaction-label" text="0" translateX="-1.0" />
   </children>
   <cursor>
      <Cursor fx:constant="HAND" />
   </cursor>
</Pane>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_RIGHT" spacing="5.0" stylesheets="@../../css/msg.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gov.iti.link.presentation.controllers.FileControllerSingle">
   <children>
      <Label fx:id="lblMessage" styleClass="right-message" textFill="WHITE">
         <graphic>
            <ImageView fitHeight="30.0" fitWidth="31.0" onMouseClicked="#OnClickFile" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../images/file.png" />
               </image>
            </ImageView>
         </graphic>
         <VBox.margin>
            <Insets right="5.0" top="5.0" />
         </VBox.margin></Label>
      <Label fx:id="lblTime" alignment="BOTTOM_RIGHT" contentDisplay="RIGHT" styleClass="labelTime" text="Time" translateX="-10.0" />
   </children>
</VBox>

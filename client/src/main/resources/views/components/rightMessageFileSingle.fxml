<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="5.0" stylesheets="@../../css/msg.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gov.iti.link.presentation.controllers.FileControllerSingle">
   <children>
      <Label fx:id="lblMessage" prefHeight="58.0" prefWidth="93.0" styleClass="right-message" textFill="WHITE">
         <graphic>
            <ImageView fitHeight="39.0" fitWidth="55.0" onMouseClicked="#OnClickFile" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../images/file.png" />
               </image>
            </ImageView>
         </graphic></Label>
      <Label fx:id="lblTime" alignment="BOTTOM_RIGHT" contentDisplay="RIGHT" text="Time" />
   </children>
</VBox>

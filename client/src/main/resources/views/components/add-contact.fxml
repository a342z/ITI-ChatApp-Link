<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonType?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<DialogPane prefHeight="172.0" prefWidth="390.0" stylesheets="@../../css/add-contact.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gov.iti.link.presentation.controllers.AddContactController">
   <content>
      <AnchorPane prefHeight="91.0" prefWidth="390.0">
         <children>
            <TextField fx:id="textfieldPhone" layoutX="14.0" layoutY="50.0" prefHeight="26.0" prefWidth="259.0" />
            <Button fx:id="buttonAddContact" layoutX="299.0" layoutY="50.0" mnemonicParsing="false" onAction="#onAddContact" prefHeight="26.0" prefWidth="86.0" styleClass="add-button" text="Add" />
            <Label layoutX="85.0" layoutY="93.0" prefHeight="18.0" prefWidth="220.0" style="-fx-alignment: center;" text="Invitation Sent Succesfully" textAlignment="CENTER" />
         </children>
      </AnchorPane>
   </content>
   <header>
      <Label style="-fx-font-weight: bold;" styleClass="header-label" text="Add New Contact" />
   </header>
   <buttonTypes>
      <ButtonType fx:constant="CLOSE" />
   </buttonTypes>
</DialogPane>
